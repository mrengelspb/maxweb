{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "consistent-return": "off",
        "react/forbid-prop-types": "off",
        "no-restricted-syntax": "off",
        "camelcase": "off",
        "radix": "off",
        "jsx-a11y/label-has-associated-control": "off",
        "space-infix-ops": "off",
        "react/require-default-props": "off"
    }
}
